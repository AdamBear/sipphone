<?xml version="1.0" encoding="UTF-8"?>
<RootPhoto>
	<!-- 图片文件存放文件夹 -->
	<Folder filter=".jpg,.jpeg,.png,.bmp">
		<Item path="Resource:" can_read="1" can_write="0" short_name="sysdata" /> 
		<Item path="Booked:" can_read="1" can_write="0" short_name="vardata" /> 
		<Item path="Config:" can_read="1" can_write="1" short_name="userdata" />
	</Folder>
	
	<!-- 图片文件的导入路径 -->
	<Import filter=".jpg,.jpeg,.png">
		<Item path="Config:"/>
	</Import>
</RootPhoto>
